{% comment %} <style>
  .login-page {
    background-image: url(https://cdn.shopify.com/s/files/1/0604/8717/2182/files/register_back.png?v=1724370399);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
  }

</style> {% endcomment %}
<div class="login-page {% if section.settings.include_margins %}{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
  {%- for block in section.blocks -%}
    {% render block %}
  {%- endfor -%}
</div>

{% schema %}
{
  "name": "t:sections.apps.name",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "checkbox",
      "id": "include_margins",
      "default": true,
      "label": "t:sections.apps.settings.include_margins.label"
    }

  ],
  "blocks": [
    {
      "type": "@app"
    }
  ],
  "presets": [
    {
      "name": "t:sections.apps.presets.name"
    }
  ]
}
{% endschema %}
